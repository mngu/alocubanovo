<!DOCTYPE html>
<html>
  <head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>{% block title %}Alocubanovo{% endblock %}</title>
    {% block stylesheets %}
      <link rel="stylesheet" href="{{ asset('css/bootstrap.css') }}" type="text/css" />
      <link rel="stylesheet" href="{{ asset('css/style.css') }}" type="text/css" />
    {% endblock %}
  </head>
  <body>
    <section id="header">
      <div class="container">
        <div class="row">
          <div class="col-md-12">
            {{ render(controller("CbnvMainBundle:Default:displayLogo")) }}
          </div>
        </div>
      </div>
    </section>
    <section id="content">
      <div class="container">
        <div class="row">
          <div class="col-md-12">
            {% block body %}
            {% endblock %}
          </div>
        </div>
      </div>
    </section>
    <section id="footer">
      <div class="container">
        <div class="row">
          <div class="col-md-12">
            <h5>Alocubanovo</h5>
            <p>
              Le moulin à café<br/>
              9, place de la Garenne<br/>
              75014 Paris métro pernety<br/>
              01.40.44.87.55
            </p>
          </div>
        </div>
      </div>
    </section>
    {% block javascripts %}
      {# Ajoutez ces lignes JavaScript si vous comptez vous servir des fonctionnalités du bootstrap Twitter #}
      <script type="text/javascript" src="{{ asset('js/jquery-1.7.2.min.js') }}"></script>
      <script type="text/javascript" src="{{ asset('js/bootstrap.js') }}"></script>
      <script type="text/javascript" src="{{ asset('js/ckeditor/ckeditor.js') }}"></script>
      <script type="text/javascript" src="{{ asset('js/init.js') }}"></script>
    {% endblock %}
  </body>
</html>